<!--此处模板template_id:58--><div id="job-iframe_5fe4_d4cf5d">
<iframe
       
src='https://www.hotjob.cn/wt/yungu/web/index/social?brandCode=1&request_locale=zh_CN&ticket=&cssLink=https%3A%2F%2Fcareer.yungu.org%2Fpublic%2Fcss%2Fhotjob.css'
       
        width='100%' height='100%' frameborder='0' class='myframe'></iframe>
<script>
    (function() {
        window.addEventListener('message', function(event) {
            if (event.origin !== 'https://www.yungu.org') {
                return;
            }
            var ph = parseInt(event.data, 10);
				    if (!isNaN(ph)) {
				        var p = document.getElementsByClassName('myframe');
				        p && (p[0].height = ph + 'px');
            }
        }, false);
      var isPC = 'false';
      if ( isPC === 'false') {
    window.location.href = 'https://www.hotjob.cn/wt/yungu/web/index';
  }
    })();
</script>
</div>


    <!-- 统计代码 -->
    <div style="display:none;">
      <script
        type="text/javascript"
        src="https://s95.cnzz.com/z_stat.php?id=1261257612&web_id=1261257612"
      ></script>
      <script>
        window.sendCZC = function(args) {
          window._czc && args && window._czc.push(args);
        };
        var pageList = window.mockDate.pageList,
          imageFooter = $(".yg-know-more .yg-footer .yg-footer-cover"),
          imageFooterMobile = $(
            ".yg-know-more .yg-footer .yg-footer-cover-mobile "
          );

        imageFooter.attr("src", pageList[0].footerImg);
        imageFooterMobile.attr("src", pageList[0].footerImgMobile);

        function changeHeaderColor() {
          var pathName = window.location.pathname;
          var headerColor = "rgba(2, 88, 191,0.85)";
          if (pathName.indexOf("primary") != -1) {
            headerColor = "rgba(63,176,199,0.85)";
          } else if (pathName.indexOf("middle") != -1) {
            headerColor = "rgba(65,146,222,0.85)";
          } else if (pathName.indexOf("kindergarten") != -1) {
            headerColor = "rgba(101,169,81, 0.85)";
          }

          $(".ygMenu").css("background", headerColor);
        }
        changeHeaderColor();
        
      </script>
    </div>
